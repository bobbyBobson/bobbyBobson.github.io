while (i < 5):
    path = './Desktop'
    file = open(join(path, q+str(i)),'w')
    file.close()
    path = './Documents'
    file = open(join(path, q+str(i)),'w')
    file.close()
    path = './Music'
    file = open(join(path, q+str(i)),'w')
